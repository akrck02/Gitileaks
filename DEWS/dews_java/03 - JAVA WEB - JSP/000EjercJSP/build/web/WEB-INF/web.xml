<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletPrepararProductos4</servlet-name>
        <servlet-class>servlets.ServletPrepararProductos4</servlet-class>
        <init-param>
            <param-name>fichproductos</param-name>
            <param-value>productos.txt</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ServletCuenta6</servlet-name>
        <servlet-class>servlets.ServletCuenta6</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMovimientos6</servlet-name>
        <servlet-class>servlets.ServletMovimientos6</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInsertar8</servlet-name>
        <servlet-class>servlets.ServletInsertar8</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletPrepararProductos4</servlet-name>
        <url-pattern>/ServletPrepararProductos4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCuenta6</servlet-name>
        <url-pattern>/ServletCuenta6</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMovimientos6</servlet-name>
        <url-pattern>/ServletMovimientos6</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInsertar8</servlet-name>
        <url-pattern>/ServletInsertar8</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
